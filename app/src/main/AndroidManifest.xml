<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.jocruzcsumb.bookrentalsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main_Menu"
            android:label="Main Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"
            android:label="Login">
        </activity>
        <activity android:name=".Create_Account"
            android:label="Create Account">
        </activity>
        <activity android:name=".Place_Hold"
            android:label="Place Hold">
        </activity>
        <activity android:name=".Cancel_Hold"
            android:label="Cancel Hold">
        </activity>
        <activity android:name=".Manage_System"
            android:label="Manage System">
        </activity>
    </application>

</manifest>